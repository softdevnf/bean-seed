{
    "server": {
        "host": "0.0.0.0",
        "port": 3000
    },
    "db": {
        "uri": "mongodb://localhost:27017/api"
    },
    "logs": {
        "level": "debug",
        "handleExceptions": false
    },
    "unauthorization": [
        "/api/v1/users/login", 
        "/api/v1/users/create",
        "/api/v1/config/get"
    ],
    "token_expire": 86400,
    "time_format": "YYYY-DD-MM H:mm:ss:SSS"
}
